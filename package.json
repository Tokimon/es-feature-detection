  },
  "devDependencies": {
    "@types/chai": "4.2.11",
    "@types/globby": "9.1.0",
    "@types/mocha": "7.0.2",
    "@types/mocha-each": "2.0.0",
    "@types/node": "^13.9.1",
    "chai": "4.2.0",
    "cross-env": "7.0.2",
    "globby": "11.0.0",
    "mocha": "7.1.0",
    "mocha-each": "2.0.1",
    "ts-node": "8.6.2",
    "tslint": "6.1.0",
