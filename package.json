{
  "name": "es-feature-detection",
  "version": "3.0.0",
  "description": "ECMAScript feature and API detection",
  "main": "index.js",
  "sideEffects": false,
  "scripts": {
    "test": "npm run build:tests && jest",
    "test:nobuild": "jest",
    "build": "npm run build:index && npm run compile",
    "build:index": "./bin/build-index.sh",
    "build:tests": "./bin/build-tests.sh",
    "compile": "tsc -p ./tsconfig.build.json",
    "compile:tests": "tsc -p ./tsconfig.tests.json --outDir \"./tests/build\"",
    "typecheck": "tsc -p ./tsconfig.build.json --noEmit",
    "clean": "npm run clean:compile && npm run clean:index && npm run clean:tests",
    "clean:compile": "tsc --build --clean",
    "clean:index": "./bin/clean-index.sh",
    "clean:tests": "./bin/clean-tests.sh",
    "prepublishOnly": "npm run build",
    "postpublish": "npm run clean"
  },
  "repository": {
    "type": "git",
    "url": "git+https://Tokimon@github.com/Tokimon/es-feature-detection.git"
  },
  "keywords": [
    "es6",
    "es7",
    "es2015",
    "es2016",
    "es2017",
    "es2018",
    "es2019",
    "es2020",
    "ecmascript",
    "feature",
    "detect",
    "check",
    "test",
    "syntax",
    "polyfill",
    "builtin",
    "dom",
    "localization"
  ],
  "author": "Toke Voltelen <tokevoltelen@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Tokimon/es-feature-detection/issues"
  },
  "homepage": "https://github.com/Tokimon/es-feature-detection#readme",
  "devDependencies": {
    "@tsconfig/node16": "^1.0.2",
    "@types/jest": "^27.0.3",
    "@types/node": "^17.0.0",
    "jest": "^27.4.5",
    "ts-jest": "^27.1.2",
    "ts-node": "^10.4.0",
    "tslint": "^6.1.3",
    "tslint-config-semistandard": "8.0.1",
    "typescript": "^4.5.4"
  }
}
